 {
  "printWidth": 80,
  "tabWidth": 2, 
  "useTabs": false, 
  "semi": false,
  "singleQuote": true, 
  "quoteProps": "as-needed",
  "jsxSingleQuote": false, 
  "trailingComma": "none",
  "bracketSpacing": true, 
  "bracketSameLine": false, 
  "arrowParens": "always",
  "requirePragma": false, 
  "insertPragma": false,
  "proseWrap": "preserve", 
  "htmlWhitespaceSensitivity": "ignore", 
  "endOfLine": "auto"
}
