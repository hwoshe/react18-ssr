  "main": "src/server/app.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production",
    "cls": "rimraf dist && rimraf node_modules",
    "dev": "rimraf dist && cross-env NODE_ENV=development SSR_MODE=stream ts-node build/build.ts",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:json": "eslint . --fix --cache --ext .json",
    "lint:package": "sort-package-json package.json",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint \"src/**/*.(tsx|jsx|css)\" --fix",
    "prepare": "husky install",
    "report": "rimraf dist && cross-env NODE_ENV=development webpack serve --config webapck/web.ts",
    "server": "tsnd -r tsconfig-paths/register src/server/app.ts",
    "start": "pnpm run dev",
    "start:csr": "rimraf dist && cross-env NODE_ENV=development SSR_MODE=none webpack serve --config webapck/web.ts"
  },
